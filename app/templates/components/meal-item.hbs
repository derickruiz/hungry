{{#link-to "meal" id}}
  <div class="Meal-photo">
      <img src="{{photo}}" alt="Photo of {{description}}">
  </div>

    <!-- <div class="Meal-chef">
        <img src="https://d3nms6niqzdkss.cloudfront.net/unsafe/smart/filters:crop_focal(300,300)/hungry-production/chef_profiles/2016/8/1/f122674d-6ef5-4e0f-bff9-adf1bc16f359-1470074631.jpg" alt="Chef photo">
    </div> -->

    <div class="Meal-content">
      <div class="Meal-name">{{name}}</div>
      <div class="Meal-location">{{location}}</div>
      <div class="Meal-description">
        <div class="Meal-price">{{price}}</div>
        <div class="Meal-rating">
            {{#if (eq rating 0) }}
            {{else}}
                <img src="data:image/svg+xml;base64,">
                {{decimalToPercentage rating}}
            {{/if}}
         </div>

             {{#if (eq reviews_count 0) }}
             {{else}}
                <div class="Meal-numberOfRatings">{{reviews_count}} ratings</div>
             {{/if}}
      </div>
    </div>
{{/link-to}}

{{yield}}
